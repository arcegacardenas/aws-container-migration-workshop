node {

   stage ('Checkout'){
       git 'https://github.com/omarlari/aws-container-migration-workshop.git'
   }

   stage ('Build Dockerfile'){
      sh 'pwd'
      dir ('Lab2'){
        sh 'pwd'
        def dockerfile = 'Dockerfile.build'
        docker.build("my-image:${env.BUILD_ID}", "-f ./${dockerfile} .")   
      }



   }
}
